{
	"trailingComma": "none",
	"useTabs":true,
	"semi": false,
	"tabWidth":2,
	"jsxSingleQuote": true,
	"singleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@components/(.*)$", 
		"^@styles/(.*)$", 
		"^@hooks/(.*)$", 
		"^@providers/(.*)$", 
		"^@services/(.*)$", 
		"^@types/(.*)$", 
		"^@store/(.*)$", 
		"^@utils/(.*)$",
		"^@../(.*)$",
		"^@./(.*)$",
		"^(.scss)$"
	],
	"importOrderSeparation": true,
  	"importOrderSortSpecifiers": true
}